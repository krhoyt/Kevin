<nav class="site-nav">
  <div class="site-nav-left">
    <a class="site-nav-item" href="/">Home</a>
    <a class="site-nav-item" href="/about">About</a>   
    <a class="site-nav-item" href="/events">Events</a>       
    <a class="site-nav-item" href="/lounge">Lounge</a>     
  </div>
  {% if collections.docs %}
    <div class="site-nav-right">
      {% for doc in collections.docs %}
        <a {% if page.url == doc.url %}aria-current="page"{% endif %} class="site-nav-button{% if doc.featured %}  site-nav-button-featured{% endif %}" href="{{ doc.url }}">
          {{ doc.meta_title if doc.meta_title else doc.title }}
        </a>
      {% endfor %}
    </div>
  {% endif %}
</nav>
