permalink: feed.xml
eleventyExcludeFromCollections: true
date: Last Modified
metadata:
  author:
    email: myemailaddress@example.com
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Kevin Hoyt</title>
  <subtitle>Makes cool shit.</subtitle>
  <link href="https://kevinhoyt.com/{{ page.url }}" rel="self"/>
  <link href="https://kevinhoyt.com"/>
  {%- for post in collections.posts.slice(0,1) | sort(true, false, 'updated_at') %}
  <updated>{{ post.updated_at }}</updated>
  {%- endfor %}
  <id>https://kevinhoyt.com/</id>
  <author>
    <name>Kevin Hoyt</name>
  </author>
  {%- for post in collections.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="https://kevinhoyt.com/{{ site.url + post.url }}"/>
    <updated>{{ post.updated_at }}</updated>
    <id>https://kevinhoyt.com/{{ post.url }}</id>
    <content type="html">{{ post.html | htmlToAbsoluteUrls('https://kevinhoyt.com/' + page.url) }}</content>
  </entry>
  {%- endfor %}
</feed>
